@model IEnumerable<ContosoUniversity.Models.SchoolViewModels.EnrollmentDateGroup>

@{
    ViewData["Title"] = "Student enrollment statistics";
}

<h2>Statistics</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Date</th>
            <th>Students</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var studentGroup in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => studentGroup.EnrollmentDate)
                </td>
                <td>
                    @studentGroup.StudentCount
                </td>
            </tr>
        }
    </tbody>
</table>